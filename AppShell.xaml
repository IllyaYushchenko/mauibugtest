<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiBugTest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiBugTest"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiBugTest">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>

    <ShellContent
        Title="Page 1"
        ContentTemplate="{DataTemplate local:Views.Page1}"
        Route="Page1"/>

    <ShellContent
        Title="Page 2"
        ContentTemplate="{DataTemplate local:Views.Page2}"
        Route="Page2"/>


</Shell>
